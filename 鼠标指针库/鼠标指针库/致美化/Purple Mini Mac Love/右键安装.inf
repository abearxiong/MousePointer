busy		= "Wait.ani" 
cross		= "Cross.cur" 
text		= "IBeam.cur" 
hand		= "Handwriting.cur" 
unavailiable	= "NO.cur" 
vert		= "SizeNS.cur" 
horz		= "SizeWE.cur" 
dgn1		= "SizeNWSE.cur" 
dgn2		= "SizeNESW.cur" 
move		= "SizeAll.cur" 
alternate	= "UpArrow.cur" 
