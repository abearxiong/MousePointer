[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%Cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%Unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"
HKLM,"SOFTWARE\Microsoft\Windows\CurrentVersion\Runonce\Setup\","",,"rundll32.exe shell32.dll,Control_RunDLL main.cpl @0,1"

[Scheme.Cur]
Arrow.cur
AppStarting.cur
Cross.cur
SizeWE.cur
Help.cur
Hand.cur
SizeAll.cur
SizeNESW.cur
SizeNS.cur
SizeNWSE.cur
Handwriting.cur
IBeam.cur
NO.cur
UpArrow.cur
Wait.cur

[Strings]
CUR_DIR     = "Cursors\Life is Strange"
SCHEME_NAME = "LiS by Max Caulfield"
alternate   = "UpArrow.cur"
busy    = "Wait.cur"
cross   = "Cross.cur"
dgn1    = "SizeNWSE.cur"
dgn2    = "SizeNESW.cur"
hand    = "Handwriting.cur"
help    = "Help.cur"
horz    = "SizeWE.cur"
link    = "Hand.cur"
move    = "SizeAll.cur"
pointer = "Arrow.cur"
text    = "IBeam.cur"
unavailiable = "NO.cur"
vert    = "SizeNS.cur"
work    = "Appstarting.cur"