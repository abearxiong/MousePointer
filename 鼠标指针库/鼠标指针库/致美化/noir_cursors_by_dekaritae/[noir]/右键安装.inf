
[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg
Scheme_Preview = [noir].bmp

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%Normal%,%10%\%CUR_DIR%\%Help%,%10%\%CUR_DIR%\%Work%,%10%\%CUR_DIR%\%Busy%,%10%\%CUR_DIR%\%Precision%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%Unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%Move%,%10%\%CUR_DIR%\%Alternate%,%10%\%CUR_DIR%\%Link%"

; -- Common Information

[Scheme.Cur]
